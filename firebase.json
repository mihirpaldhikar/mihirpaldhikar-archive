{
  "hosting": {
    "appAssociation": "AUTO",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "mihirpaldhikar-portfolio",
          "region": "asia-south1"
        }
      },
      {
        "source": "/links/**",
        "dynamicLinks": true
      }
    ]
  }
}
